									obj.attName = attName;
									obj.addressArray = selectedFeature.attributes[attName] as ArrayList;
									addressViewer.addressObjectArray.addItem(obj);

									addressViewer.addEventListener(Event.CLOSE, addressViewerClose);						
								}
					

<opDef name="GeoOperations">	<opSet setName="Center">		<operation opeName="centerOfPoints" returnAttributeType="SG_Point">			<argument argName="points" dataType="SG_Point" multiplicity="true"/>		</operation>		<operation opeName="centerOfCurve" returnAttributeType="SG_Point">			<argument argName="curve" dataType="SG_Curve" multiplicity="false"/>		</operation>		<operation opeName="centerOfSurface" returnAttributeType="SG_Point">			<argument argName="surface" dataType="SG_Surface" multiplicity="false"/>		</operation>		<operation opeName="centerOfComplex" returnAttributeType="SG_Point">			<argument argName="complex" dataType="SG_Complex" multiplicity="false"/>		</operation>		<operation opeName="centerLayingOnCurve" returnAttributeType="SG_Point">			<argument argName="curve" dataType="SG_Curve" multiplicity="false"/>		</operation>		<operation opeName="centerOfMIC" returnAttributeType="SG_Point">			<argument argName="surface" dataType="SG_Surface" multiplicity="false"/>		</operation>				<operation opeName="locateFacility" returnAttributeType="SG_Point">			<argument argName="surfaces" dataType="SG_Surface" multiplicity="true"/>			<argument argName="stories" dataType="Integer" multiplicity="true"/>					</operation>					</opSet>	<opSet setName="Length">		<operation opeName="lengthOfCurve" returnAttributeType="Real">			<argument argName="curve" dataType="SG_Curve" multiplicity="false"/>		</operation>	</opSet>	<opSet setName="DistanceBetween">		<operation opeName="distancePtoP" returnAttributeType="Real">			<argument argName="fromPoint" dataType="SG_Point" multiplicity="false"/>			<argument argName="toPoint" dataType="SG_Point" multiplicity="false"/>		</operation>		<operation opeName="distancePtoC" returnAttributeType="Real">			<argument argName="fromPoint" dataType="SG_Point" multiplicity="false"/>			<argument argName="toCurve" dataType="SG_Curve" multiplicity="false"/>		</operation>		<operation opeName="distanceCtoC" returnAttributeType="Real">			<argument argName="fromCurve" dataType="SG_Curve" multiplicity="false"/>			<argument argName="toCurve" dataType="SG_Curve" multiplicity="false"/>		</operation>		<operation opeName="distanceCtoS" returnAttributeType="Real">			<argument argName="fromCurve" dataType="SG_Curve" multiplicity="false"/>			<argument argName="toSurface" dataType="SG_Surface" multiplicity="false"/>		</operation>	</opSet>	<opSet setName="ConvexHull">		<operation opeName="get" returnAttributeType="SG_Surface">			<argument argName="points" dataType="SG_Point" multiplicity="true"/>		</operation>	</opSet>	<opSet setName="SurfaceArea">		<operation opeName="getArea" returnAttributeType="Real">			<argument argName="surface" dataType="SG_Surface" multiplicity="false"/>		</operation>	</opSet>	<opSet setName="Buffer">		<operation opeName="getOnBufferPoints" returnAttributeType="Bool">			<argument argName="baseCurve" dataType="SG_Curve" multiplicity="false"/>			<argument argName="maxDistance" dataType="Real" multiplicity="false"/>						<argument argName="points" dataType="SG_Point" multiplicity="true"/>		</operation>		<operation opeName="getOnBufferCurves" returnAttributeType="Bool">			<argument argName="baseCurve" dataType="SG_Curve" multiplicity="false"/>			<argument argName="maxDistance" dataType="Real" multiplicity="false"/>						<argument argName="curves" dataType="SG_Curve" multiplicity="true"/>		</operation>		<operation opeName="getOnBufferSurfaces" returnAttributeType="Bool">			<argument argName="baseCurve" dataType="SG_Curve" multiplicity="false"/>			<argument argName="maxDistance" dataType="Real" multiplicity="false"/>						<argument argName="surfaces" dataType="SG_Surface" multiplicity="true"/>		</operation>	</opSet>	<opSet setName="Ratio">		<operation opeName="getPlotRatio" returnAttributeType="Real">			<argument argName="landShape" dataType="SG_Surface" multiplicity="false"/>			<argument argName="buildingShape" dataType="SG_Surface" multiplicity="false"/>			<argument argName="stories" dataType="Integer" multiplicity="false"/>		</operation>	</opSet></opDef>